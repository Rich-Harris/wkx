{
  "name": "wkx",
  "version": "0.4.5",
  "description": "A WKT/WKB/EWKT/EWKB/TWKB/GeoJSON parser and serializer",
  "main": "dist/wkx.js",
  "module": "dist/wkx.esm.js",
  "types": "lib/wkx.d.ts",
  "files": [
    "dist/",
    "lib/"
  ],
  "scripts": {
    "test": "jshint . && mocha -r esm",
    "pretest": "npm run build",
    "build": "rollup -c && terser -cm -- ./dist/wkx.js > ./dist/wkx.min.js",
    "prepare": "npm run build",
    "coveralls": "istanbul cover node_modules/mocha/bin/_mocha -- -r esm -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "author": "Christian Schwarz",
  "license": "MIT",
  "devDependencies": {
    "async": "^2.6.0",
    "coveralls": "^3.0.1",
    "deep-eql": "^3.0.1",
    "esm": "^3.0.84",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.5",
    "json-stringify-pretty-compact": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.1.1",
    "pg": "^7.4.3",
    "rollup": "^0.66.2",
    "terser": "^3.8.2"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/cschwarz/wkx.git"
  },
  "keywords": [
    "wkt",
    "wkb",
    "ewkt",
    "ewkb",
    "twkb",
    "geojson",
    "ogc",
    "geometry",
    "geography",
    "spatial"
  ],
  "dependencies": {
    "@types/node": "*"
  }
}
